import Foundation
import Glibc

// you can write to stdout for debugging purposes, e.g.
// print("this is a debug message")

public func solution(_ A : inout [Int]) -> Int {
    // write your code in Swift 3.0 (Linux)
    var countA:[Int:Bool] = [:] 
    var i = 1
    
    for i in 0..<A.count{
        countA[A[i]] = true
    }
    
    while true{
        if countA[i] == nil{
            return i
        }
    
    i += 1
    }
    
}